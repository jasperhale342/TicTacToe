<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="284.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
   <children>
      <Button layoutX="14.0" layoutY="166.0" mnemonicParsing="false" onAction="#createClient" prefHeight="98.0" prefWidth="280.0" text="Create Client">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button layoutX="306.0" layoutY="166.0" mnemonicParsing="false" onAction="#end" prefHeight="98.0" prefWidth="280.0" text="End">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="38.0" layoutY="22.0" prefHeight="69.0" prefWidth="497.0" text="Welcome to TicTacToe!" textAlignment="CENTER">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <Label fx:id="helpMsg" layoutX="38.0" layoutY="84.0" prefHeight="69.0" prefWidth="521.0" text="Select one of the following options. WARNING: If you click End, this will end all clients as well." wrapText="true" />
   </children>
</AnchorPane>
